.PHONY = main clean

main: func.o my_code.o
		@echo "linking main.o and library.o and generating binary"
		gcc -o library func.o my_code.o

func.o: func.c
		@echo "compiling library.c into .object file"
		gcc -c func.c

my_code.o: my_code.c
		@echo "compiling main.c into .object file"
		gcc -c my_code.c

clean: 
		@echo "cleaning build files"
		rm my_code.o func.o library
